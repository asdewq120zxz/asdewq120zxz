-- Executor Version - Weapon Auto Attack GUI (FAST SCAN)
-- ‡∏ß‡∏≤‡∏á‡πÉ‡∏ô Executor ‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local mouse = player:GetMouse()

-- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡πÄ‡∏õ‡πá‡∏ô‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠
if not UserInputService.TouchEnabled then
    warn("‚ö†Ô∏è ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ô‡∏µ‡πâ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô!")
    return
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á GUI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "MobileWeaponGUI"
ScreenGui.IgnoreGuiInset = true
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false

-- Frame ‡∏´‡∏•‡∏±‡∏Å
local MainButton = Instance.new("Frame")
MainButton.Name = "AttackButton"
MainButton.Size = UDim2.new(0, 85, 0, 85)
MainButton.Position = UDim2.new(0.85, 0, 0.75, 0)
MainButton.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
MainButton.BackgroundTransparency = 0.25
MainButton.BorderSizePixel = 0
MainButton.Active = true

-- ‡∏°‡∏∏‡∏°‡πÇ‡∏Ñ‡πâ‡∏á‡∏°‡∏ô
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0.35, 0)
UICorner.Parent = MainButton

-- ‡πÄ‡∏á‡∏≤
local UIStroke = Instance.new("UIStroke")
UIStroke.Thickness = 2
UIStroke.Color = Color3.fromRGB(255, 255, 255)
UIStroke.Parent = MainButton

-- ‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°
local StatusText = Instance.new("TextLabel")
StatusText.Name = "Status"
StatusText.Size = UDim2.new(1, 0, 1, 0)
StatusText.BackgroundTransparency = 1
StatusText.Text = "‡∏õ‡∏¥‡∏î"
StatusText.TextColor3 = Color3.fromRGB(255, 255, 255)
StatusText.Font = Enum.Font.GothamBlack
StatusText.TextSize = 22
StatusText.TextStrokeTransparency = 0.5
StatusText.Parent = MainButton

ScreenGui.Parent = player:WaitForChild("PlayerGui")
MainButton.Parent = ScreenGui

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏° (‡∏õ‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡πá‡∏ô local ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß)
local isEnabled = false
local isDragging = false
local dragStart, startPos
local attackConnection
local currentTools = {}
local lastScanTime = 0
local scanCooldown = 0.1
local lastAttackTime = 0
local attackSpeed = 0.1

-- ‚ö° ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
local isPositionLocked = false
local lockTimer = 0
local lockStartTime = 0
local isCountingForLock = false

-- ‚ö° **‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö TP Walk ‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà** (‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß 0.9 ‡∏ï‡∏≤‡∏¢‡∏ï‡∏±‡∏ß)
local tpWalkEnabled = false
local tpWalkConnection

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡πÅ‡∏Å‡∏ô‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡πÅ‡∏ö‡∏ö‡πÄ‡∏£‡πá‡∏ß (Optimized)
local function fastScanWeapons()
    currentTools = {}
    
    local character = player.Character
    if not character then return {} end
    
    for _, item in pairs(character:GetChildren()) do
        if item:IsA("Tool") then
            currentTools[#currentTools + 1] = item
        end
    end
    
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        local equippedTool = humanoid:FindFirstChildOfClass("Tool")
        if equippedTool and not table.find(currentTools, equippedTool) then
            currentTools[#currentTools + 1] = equippedTool
        end
    end
    
    task.spawn(function()
        local toolCount = #currentTools
        if isEnabled then
            StatusText.Text = "‡πÄ‡∏õ‡∏¥‡∏î\n"..toolCount.." ‡∏≠‡∏±‡∏ô"
        else
            StatusText.Text = "‡∏õ‡∏¥‡∏î\n"..toolCount.." ‡∏≠‡∏±‡∏ô"
        end
    end)
    
    return currentTools
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÄ‡∏£‡πá‡∏ß (Optimized)
local function fastAttack()
    if not isEnabled or #currentTools == 0 then return end
    
    local now = tick()
    if now - lastAttackTime < attackSpeed then return end
    lastAttackTime = now
    
    for i = 1, 3 do
        for _, weapon in ipairs(currentTools) do
            if weapon and weapon.Parent then
                pcall(function()
                    weapon:Activate()
                end)
                
                pcall(function()
                    local clicker = weapon:FindFirstChildWhichIsA("ClickDetector")
                    if clicker then
                        fireclickdetector(clicker)
                    end
                end)
            end
        end
        
        if i < 3 then
            task.wait(0.02)
        end
    end
end

-- ‚ö° **‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î TP Walk ‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà**
local function toggleTPWalk()
    tpWalkEnabled = not tpWalkEnabled
    
    if tpWalkEnabled then
        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á connection ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö TP Walk
        tpWalkConnection = RunService.Heartbeat:Connect(function()
            local char = player.Character
            if char then
                local hum = char:FindFirstChildOfClass("Humanoid")
                if hum and hum.MoveDirection.Magnitude > 0 then
                    -- ‚ö° **‡πÉ‡∏ä‡πâ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß 0.9 ‡∏ï‡∏≤‡∏¢‡∏ï‡∏±‡∏ß** ‚ö°
                    local speed = 0.9
                    char:TranslateBy(hum.MoveDirection * speed)
                end
            end
        end)
        
        print("‚úÖ TP Walk ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß (‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß 0.9)")
    else
        -- ‡∏õ‡∏¥‡∏î TP Walk
        if tpWalkConnection then
            tpWalkConnection:Disconnect()
            tpWalkConnection = nil
        end
        
        print("‚ùå TP Walk ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß")
    end
end

-- ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÇ‡∏´‡∏°‡∏î‡πÇ‡∏à‡∏°‡∏ï‡∏µ (‡πÄ‡∏£‡πá‡∏ß)
local function toggleAttackMode()
    isEnabled = not isEnabled
    
    if isEnabled then
        MainButton.BackgroundColor3 = Color3.fromRGB(50, 220, 50)
        StatusText.Text = "‡πÄ‡∏õ‡∏¥‡∏î\n"..#currentTools.." ‡∏≠‡∏±‡∏ô"
        
        attackConnection = RunService.Heartbeat:Connect(function(deltaTime)
            if isEnabled then
                fastAttack()
            end
        end)
        
        coroutine.wrap(function()
            local trans = 0.15
            while isEnabled do
                MainButton.BackgroundTransparency = trans
                trans = trans == 0.15 and 0.25 or 0.15
                task.wait(0.3)
            end
            MainButton.BackgroundTransparency = 0.25
        end)()
        
    else
        MainButton.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
        StatusText.Text = "‡∏õ‡∏¥‡∏î\n"..#currentTools.." ‡∏≠‡∏±‡∏ô"
        
        if attackConnection then
            attackConnection:Disconnect()
            attackConnection = nil
        end
        
        MainButton.BackgroundTransparency = 0.25
    end
end

-- ‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡∏≤‡∏Å‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏¢‡πâ‡∏≤‡∏¢ (‡πÄ‡∏£‡πá‡∏ß) - ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡πÉ‡∏´‡πâ‡∏Ñ‡∏≥‡∏ô‡∏∂‡∏á‡∏ñ‡∏∂‡∏á‡∏Å‡∏≤‡∏£‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
local function updateDrag(input)
    if not isDragging or isPositionLocked then return end
    
    local delta = input.Position - dragStart
    MainButton.Position = UDim2.new(
        startPos.X.Scale,
        startPos.X.Offset + delta.X,
        startPos.Y.Scale,
        startPos.Y.Offset + delta.Y
    )
end

-- ‚ö° ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
local function togglePositionLock()
    isPositionLocked = not isPositionLocked
    
    if isPositionLocked then
        -- ‡πÅ‡∏™‡∏î‡∏á‡∏ß‡πà‡∏≤‡∏•‡πá‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß (‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏™‡∏µ‡∏ô‡πâ‡∏≥‡πÄ‡∏á‡∏¥‡∏ô)
        MainButton.BackgroundColor3 = Color3.fromRGB(50, 100, 220)
        StatusText.Text = "‡∏•‡πá‡∏≠‡∏Å"
        
        -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏ß‡∏¥‡∏ö‡∏ß‡∏±‡∏ö‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏•‡πá‡∏≠‡∏Å
        coroutine.wrap(function()
            for i = 1, 3 do
                MainButton.BackgroundTransparency = 0.1
                task.wait(0.1)
                MainButton.BackgroundTransparency = 0.25
                task.wait(0.1)
            end
            
            -- ‡∏Ñ‡∏∑‡∏ô‡∏™‡∏µ‡∏ï‡∏≤‡∏°‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÇ‡∏à‡∏°‡∏ï‡∏µ
            if isEnabled then
                MainButton.BackgroundColor3 = Color3.fromRGB(50, 220, 50)
                StatusText.Text = "‡πÄ‡∏õ‡∏¥‡∏î\n"..#currentTools.." ‡∏≠‡∏±‡∏ô"
            else
                MainButton.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
                StatusText.Text = "‡∏õ‡∏¥‡∏î\n"..#currentTools.." ‡∏≠‡∏±‡∏ô"
            end
        end)()
        
        print("üîí ‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á GUI ‡πÅ‡∏•‡πâ‡∏ß")
    else
        -- ‡∏Ñ‡∏∑‡∏ô‡∏™‡∏µ‡∏ï‡∏≤‡∏°‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÇ‡∏à‡∏°‡∏ï‡∏µ
        if isEnabled then
            MainButton.BackgroundColor3 = Color3.fromRGB(50, 220, 50)
            StatusText.Text = "‡πÄ‡∏õ‡∏¥‡∏î\n"..#currentTools.." ‡∏≠‡∏±‡∏ô"
        else
            MainButton.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
            StatusText.Text = "‡∏õ‡∏¥‡∏î\n"..#currentTools.." ‡∏≠‡∏±‡∏ô"
        end
        
        print("üîì ‡∏õ‡∏•‡∏î‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á GUI ‡πÅ‡∏•‡πâ‡∏ß")
    end
end

-- ‡∏Å‡∏≤‡∏£‡πÅ‡∏ï‡∏∞‡πÅ‡∏•‡∏∞‡∏•‡∏≤‡∏Å (Optimized) - ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏à‡∏±‡∏ö‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
MainButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.Touch then
        local touchTime = tick()
        local touchPos = input.Position
        
        -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏à‡∏±‡∏ö‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á (2 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ)
        isCountingForLock = true
        lockStartTime = touchTime
        
        -- ‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á ‡πÉ‡∏´‡πâ‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏•‡∏≤‡∏Å‡πÑ‡∏î‡πâ
        if not isPositionLocked then
            isDragging = true
            dragStart = input.Position
            startPos = MainButton.Position
        end
        
        MainButton.BackgroundTransparency = 0.4
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                isDragging = false
                isCountingForLock = false
                MainButton.BackgroundTransparency = isEnabled and 0.15 or 0.25
                
                -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÅ‡∏ï‡∏∞‡∏´‡∏£‡∏∑‡∏≠‡∏•‡∏≤‡∏Å
                local duration = tick() - touchTime
                local distance = (input.Position - touchPos).magnitude
                
                -- ‡∏ñ‡πâ‡∏≤‡πÅ‡∏ï‡∏∞‡∏™‡∏±‡πâ‡∏ô‡πÜ (‡πÑ‡∏°‡πà‡∏ñ‡∏∂‡∏á 2 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ) ‡πÅ‡∏•‡∏∞‡πÑ‡∏°‡πà‡∏•‡∏≤‡∏Å‡∏°‡∏≤‡∏Å = ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÇ‡∏´‡∏°‡∏î‡πÇ‡∏à‡∏°‡∏ï‡∏µ
                if duration < 0.2 and distance < 10 then
                    toggleAttackMode()
                end
            end
        end)
    end
end)

-- ‚ö° ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Å‡∏≤‡∏£‡∏Å‡∏î‡∏Ñ‡πâ‡∏≤‡∏á 2 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡πá‡∏≠‡∏Å/‡∏õ‡∏•‡∏î‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
RunService.Heartbeat:Connect(function(deltaTime)
    if isCountingForLock then
        local currentTime = tick()
        local holdDuration = currentTime - lockStartTime
        
        -- ‡∏ñ‡πâ‡∏≤‡∏Å‡∏î‡∏Ñ‡πâ‡∏≤‡∏á‡πÄ‡∏Å‡∏¥‡∏ô 2 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
        if holdDuration >= 2 then
            isCountingForLock = false
            togglePositionLock()
        end
    end
end)

-- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó‡∏Å‡∏≤‡∏£‡∏•‡∏≤‡∏Å
UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.Touch and isDragging then
        updateDrag(input)
    end
end)

-- ‚ö° ‡∏™‡πÅ‡∏Å‡∏ô‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡πÅ‡∏ö‡∏ö‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î
RunService.Heartbeat:Connect(function(deltaTime)
    local now = tick()
    if now - lastScanTime >= scanCooldown then
        lastScanTime = now
        fastScanWeapons()
    end
end)

-- ‡∏™‡πÅ‡∏Å‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£ (‡πÄ‡∏£‡πá‡∏ß)
player.CharacterAdded:Connect(function()
    task.wait(0.5)
    fastScanWeapons()
end)

-- ‡∏™‡πÅ‡∏Å‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠ Equip/Unequip Tool (‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î)
local function setupToolListener()
    local backpack = player:WaitForChild("Backpack")
    
    local function immediateScan()
        fastScanWeapons()
    end
    
    backpack.ChildAdded:Connect(immediateScan)
    backpack.ChildRemoved:Connect(immediateScan)
    
    player.CharacterAdded:Connect(function(char)
        char.ChildAdded:Connect(immediateScan)
        char.ChildRemoved:Connect(immediateScan)
    end)
end

-- ‚ö° **‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏õ‡∏∏‡πà‡∏°‡∏Ñ‡∏µ‡∏¢‡πå‡∏ö‡∏≠‡∏£‡πå‡∏î‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö TP Walk** (‡πÅ‡∏ï‡∏∞‡∏¢‡∏≤‡∏ß 2 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ï‡∏¥‡∏î‡∏Å‡∏±‡∏ô)
local lastTPWalkTime = 0
MainButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.Touch then
        local currentTime = tick()
        
        -- ‡∏ñ‡πâ‡∏≤‡∏Å‡∏î‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏™‡∏≠‡∏á‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏Å‡∏¥‡∏ô 0.5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ = ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î TP Walk
        if currentTime - lastTPWalkTime < 0.5 then
            toggleTPWalk()
            lastTPWalkTime = 0
        else
            lastTPWalkTime = currentTime
        end
    end
end)

-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏™‡πÅ‡∏Å‡∏ô‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
task.spawn(function()
    fastScanWeapons()
    setupToolListener()
end)

print("‚ö° Mobile Weapon GUI ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à! (‡∏°‡∏µ‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á)")
print("üì± ‡∏Å‡∏î‡∏Ñ‡πâ‡∏≤‡∏á 2 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ: ‡∏•‡πá‡∏≠‡∏Å/‡∏õ‡∏•‡∏î‡∏•‡πá‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á GUI")
print("üëâ ‡πÅ‡∏ï‡∏∞‡∏™‡∏±‡πâ‡∏ô‡πÜ: ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÇ‡∏´‡∏°‡∏î‡πÇ‡∏à‡∏°‡∏ï‡∏µ")
print("üëâ ‡πÅ‡∏ï‡∏∞‡∏Ñ‡πâ‡∏≤‡∏á‡πÅ‡∏•‡∏∞‡∏•‡∏≤‡∏Å: ‡∏¢‡πâ‡∏≤‡∏¢‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á (‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡∏•‡πá‡∏≠‡∏Å)")
print("‚ö° ‡πÅ‡∏ï‡∏∞ 2 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏£‡πá‡∏ß‡πÜ: ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î TP Walk (‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß 0.9)")
